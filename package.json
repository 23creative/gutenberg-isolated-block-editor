{
	"name": "isolated-block-editor",
	"version": "1.1.0",
	"description": "Repackages Gutenberg's editor playground as multi-instance editor",
	"main": "build/index.js",
	"module": "build-module/index.js",
	"scripts": {
		"start": "BUILD_ENV=es6 babel src --out-dir build-module --source-maps --ignore 'src/**/__tests__/*.js' --copy-files --no-copy-ignored --watch",
		"es6": "BUILD_ENV=es6 babel src --out-dir build-module --source-maps --ignore 'src/**/__tests__/*.js' --copy-files --no-copy-ignored",
		"cjs": "BUILD_ENV=cjs babel src --out-dir build --source-maps --ignore 'src/**/__tests__/*.js' --copy-files --no-copy-ignored",
		"browser": "NODE_ENV=production webpack --mode production --progress --config ./webpack.config.browser.js",
		"build": "yarn es6 && yarn cjs && yarn browser",
		"clean": "rm -rf build build-module build-browser"
	},
	"sideEffects": [
		"*.css",
		"*.scss"
	],
	"author": "Automattic",
	"license": "GPL-2.0-or-later",
	"dependencies": {
		"@wordpress/a11y": "^3.1.0",
		"@wordpress/annotations": "^2.1.0",
		"@wordpress/api-fetch": "^5.1.0",
		"@wordpress/autop": "^3.1.0",
		"@wordpress/blob": "^3.1.0",
		"@wordpress/block-editor": "^6.1.0",
		"@wordpress/block-library": "^3.2.0",
		"@wordpress/block-serialization-default-parser": "^4.1.0",
		"@wordpress/block-serialization-spec-parser": "^4.1.0",
		"@wordpress/blocks": "^9.1.0",
		"@wordpress/components": "^14.1.0",
		"@wordpress/compose": "^4.1.0",
		"@wordpress/core-data": "^3.1.0",
		"@wordpress/data": "^5.1.0",
		"@wordpress/data-controls": "^2.1.0",
		"@wordpress/date": "^4.1.0",
		"@wordpress/deprecated": "^3.1.0",
		"@wordpress/dom": "^3.1.0",
		"@wordpress/dom-ready": "^3.1.0",
		"@wordpress/editor": "^10.1.0",
		"@wordpress/element": "^3.1.0",
		"@wordpress/escape-html": "^2.1.0",
		"@wordpress/format-library": "^2.1.0",
